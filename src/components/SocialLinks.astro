---
import networks from '@/data/networks.json'
import ExternalLink from "./ExternalLink.astro";
import SocialIcon from './SocialIcon.astro';
---

{networks.filter(p => !p.isSecondary).map(item => (
  <ExternalLink class="icon" href={item.url} title={item.name}>
    <SocialIcon class="size-6" name={item.name.toLowerCase()} />
  </ExternalLink>
))}
