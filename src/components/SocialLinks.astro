---
import { basics } from '~/resume.json'
import ExternalLink from "./ExternalLink.astro";
import SocialIcon from './SocialIcon.astro';
---

{basics.profiles.filter(p => !p.isSecondary).map(item => (
  <ExternalLink class="icon" href={item.url} title={item.network}>
    <SocialIcon class="size-6" name={item.network.toLowerCase()} />
  </ExternalLink>
))}
